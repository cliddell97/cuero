.nav-1::before{
	margin: 2px;
	content:url(../images/heart15px.png);
}
.nav-1::after{
    margin: 2px;
	content: url(../images/heart15px.png) !important;
	display: inline !important;
    position: initial !important;
}
#ui-icon-carat-1-e::after{
	margin: 10px;
	position: relative !important;
}
	
.level0{
    border: none !important;
	box-shadow: none !important;
}	
.level1{
    border: none !important;
	box-shadow: none !important;
	text-align: left !important;
}	
.level2{
    border: none !important;
    box-shadow: none !important;
}	
.level1:hover{
    background: white !important;
	box-shadow: none !important;
}	
.level2:hover{
    background: white !important;
	box-shadow: none !important;
}	
.ui-icon-carat-1-e{
	display: none !important;
}

img{
	transition: 0.3s;
}
img:hover{
	opacity: 0.7;
}
